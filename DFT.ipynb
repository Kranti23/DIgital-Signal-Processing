   "source": [
    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "x=np.array([1+0j,2+0j,3+0j,4+0j])\n",
    "X=np.zeros(4,dtype=np.complex)\n",
    "N=4\n",
    "for k in range(N):\n",
    "    for n in range(N):\n",
    "        ang=-(2*np.pi*k*n)/N\n",
    "    X[k]=X[k]+(x[n]*np.cos(ang))+1j*(x[n]*np.sin(ang))\n",
    "X=np.around(X)\n",
    "print(\"DFT:\")\n",
    "print(X)\n"
   ]
